[user]
	name = Alex Towe
	email = 107151439+alextowe@users.noreply.github.com
	signingKey = ~/.ssh/id_ed25519.pub

[gpg]
	format = ssh

[init]
	defaultBranch = main

[alias]
	a = add --all
	b = branch
	cm = commit -S -m
	co = checkout
	cob = checkout -b
	com = checkout main
	l = log --oneline --graph --decorate
	pl = pull
	ps = push
	pso = push -m origin main
	s = status
[color]
	ui = auto

[color "status"]
	added = green bold
	changed = yellow bold
	untracked = red bold
